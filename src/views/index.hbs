<div style="padding: 4em;">
	<div class="row animate-box" style="height: 50px">
		<div class="col-md-6 col-md-offset-3 text-center fh5co-heading">
			<h3>Khóa Học Nổi Bật Nhất Tuần Qua</h3>
		</div>
	</div>
	<div id="carouselId" class="carousel slide" data-ride="carousel" data-interval="2000">
		<div class="carousel-inner" role="listbox">
			<div class="item active">
				<div class="row">
					{{#each highlightCourse}}
					<div class="col-md-3 col-sm-6 text-center animate-box">
						<div class="card h-100">
							<img src="../design/images/course_{{this.id}}.jpg" alt="{{this.name}}"
								title="{{this.name}}" class="card-img-top">
							<div class="card-body">
								<h6 class="card-title">{{this.name}}</h6>
								<h5 class="card-title text-danger">{{format_number this.price}}</h5>
								<p
									style="vertical-align: middle;text-align: center; margin: 0 15px; max-width:240px; white-space: nowrap;overflow: hidden;text-overflow: ellipsis;">
									{{this.tinyDes}}</p>
							</div>
							<div class="card-footer text-muted">
								<a class="btn btn-sm btn-outline-primary" href="/courses/detail/{{this.id}}"
									role="button">
									<i class="fa fa-eye" aria-hidden="true"></i>
									Details
								</a>
								{{!-- {{#if ../auth}} lấy auth của hiện tại --}}
								<a data-id="{{this.id}}" class="btn btn-sm btn-outline-success addItem"
									href="javascript:;" role="button">
									<i class="fa fa-cart-plus" aria-hidden="true"></i>
									Add to cart
								</a>
								{{!-- {{/if}} --}}
							</div>
						</div>
					</div>
					{{/each}}
				</div>
			</div>
		</div>
	</div>
</div>

